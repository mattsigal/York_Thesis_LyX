#% Do not delete the line below; configure depends on this
#  \DeclareLaTeXClass[york-thesis,apacite.sty]{book (york-thesis)}

Format 35
Columns                 1
Sides                   1
PageStyle               Headers


ClassOptions
	Other "12pt, final"
	FontSize              10|11|12
End


Preamble
    \usepackage{fancyhdr}
    \lhead{}
    \chead{}
    \rhead{}
    \lfoot{}
    \cfoot{\thepage}
    \rfoot{}
    \renewcommand{\headrulewidth}{0pt}
    \renewcommand{\footrulewidth}{0pt}
    \renewcommand\headheight{24pt}
    \renewcommand\maketitle{}
EndPreamble


Style Standard
	LatexName             dummy
	LatexType             Paragraph
	Margin                Static
	ParIndent             MM
	ParSkip               0.4
	ParSep                0.4
	TopSep                1.0
	BottomSep             1.0
	Align                 Block
	AlignPossible         Block, Left, Right, Center
	LabelType             No_Label
End


Style Title
	Category              FrontMatter
	LatexName             title
	LatexType             Command
	InTitle               1
	Font
	  Size                Huge
	EndFont
	Align                 Center
	HTMLTitle             true
End

Style DegreeName
	Category              FrontMatter
	LatexName             degreename
	LatexType             Command
	InTitle               1
	Align                 Center
	HTMLTitle             true
End

Style Author
	Category              FrontMatter
	LatexName             author
	LatexType             Command
	InTitle               1
	Align                 Center
	AlignPossible         Left, Right, Center
	TopSep                0.5
	BottomSep             0.5
	Font
	  Shape               Italic
	  Size                Small
	EndFont
End

Style DefenceDate 
	Category              FrontMatter
	LatexName             date
	LatexType             Command
	InTitle               1
	Align                 Center
	AlignPossible         Left, Right, Center
	TopSep                0.5
	BottomSep             0.5
	Font
	  Size                Small
	EndFont
End

Style Department
	Category              FrontMatter
	LatexName             department
	LatexType             Command
	InTitle               1
	TopSep                0.5
	ParSep                0.5
	Align                 Center
	Font
	  Size                Small
	EndFont
End

Style Abstract
	Category              FrontMatter
	LatexName             Abstract
	LatexType             Command
	InTitle               1
	Margin                Static
	ParSkip               0.4
	BottomSep             1.5
	TopSep                1.5
	ParSep                1.5
	LeftMargin            MMN
	RightMargin           MMN
	TopSep                0.8
	BottomSep             0.8
	Align                 Block
	AlignPossible         Block, Left, Right, Center
	Labeltype             Above
	LabelString           "Abstract"
	Labelsep              M
	LabelFont
	  Series              Bold
	  Size                Small
	EndFont
End

Style Acknowledgements
	Category              FrontMatter
	LatexName             Acknowledgements
	LatexType             Command
	InTitle               1
	Margin                Static
	ParSkip               0.4
	BottomSep             1.5
	TopSep                1.5
	ParSep                1.5
	Align                 Left
	LeftMargin            MMN
	RightMargin           MMN
	Align                 Block
	AlignPossible         Block, Left, Right, Center
	Labeltype             Centered
	LabelString           "Acknowledgements"
	Labelsep              M
	LabelFont
	  Series              Bold
	  Size                Small
	EndFont
End

Style Dedication
	Category              FrontMatter
	LatexName             Dedication
	LatexType             Command
	InTitle               1
	Margin                Static
	ParSkip               0.4
	BottomSep             1.5
	TopSep                1.5
	ParSep                1.5
	Align                 Left
	LeftMargin            MMN
	RightMargin           MMN
	Align                 Block
	AlignPossible         Block, Left, Right, Center
	Labeltype             Centered
	LabelString           "Dedication"
	Labelsep              M
	LabelFont
	  Series              Bold
	  Size                Small
	EndFont
End

Style Preface
	Category              ExtraFrontMatter
	LatexName             Preface
	LatexType             Command
	InTitle               1
	Margin                Static
	ParSkip               0.4
	BottomSep             1.5
	TopSep                1.5
	ParSep                1.5
	Align                 Left
	LeftMargin            MMN
	RightMargin           MMN
	Align                 Block
	AlignPossible         Block, Left, Right, Center
	Labeltype             Centered
	LabelString           "Preface"
	Labelsep              M
	LabelFont
	  Series              Bold
	  Size                Small
	EndFont
End

Style Symbols
	Category              FrontMatter
	LatexName             Symbols
	LatexType             Command
	InTitle               1
	Margin                Static
	ParSkip               0.4
	BottomSep             1.5
	TopSep                1.5
	ParSep                1.5
	Align                 Left
	LeftMargin            MMN
	RightMargin           MMN
	Align                 Block
	AlignPossible         Block, Left, Right, Center
	Labeltype             Centered
	LabelString           "List of Symbols"
	Labelsep              M
	LabelFont
	  Series              Bold
	  Size                Small
	EndFont
End

Input stdlists.inc
Input stdlayouts.inc
Input stdsections.inc
Input stdinsets.inc
Input stdfloats.inc
Input stdcounters.inc

NoStyle Paragraph
NoStyle Subparagraph

Style Chapter
	LabelType             No_Label
	Align                 Center
	Font
	  Series              Bold
	  Size                Huge
	EndFont
		TocLevel 1
End

Style Section
	LabelType             No_Label
	Font
	  Series              Bold
	  Size                Larger
	EndFont
		TocLevel 1
End


Style Subsection
	LabelType             No_Label
	Font
	  Series              Bold
	  Size                Large
	EndFont
		TocLevel 2
End


Style Subsubsection
	LabelType             No_Label
	Font
	  Series              Italic
	EndFont
		TocLevel 3
End



